generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Bucekt {
  id        Int      @id @default(autoincrement())
  status    String
  number    Int
  createdAt DateTime @default(now()) @map("created_at")

  @@map("buckets")
}
